{
    "extends": ["airbnb","prettier","plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier":[
            "error",
            {
              "endOfLine": "off"
            }
          ],
        "func-names":"off",
        "no-unused-vars":"warn",
        "no-console":"off",
        "no-process-exit":"off",
        "object-shorthand":"off",
        "class-methods-use-this":"off"
    }
}
